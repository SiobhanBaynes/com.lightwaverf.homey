          },
          "options": {
            "svg": "./assets/svg/remote.pair.svg"
          }
        },
        {
          "id": "rf.test_switch",
          "navigation": {
            "prev": "rf.imitate",
            "next": "choose_slave"
          }
        },
        {
          "template": "choose_slave",
          "id": "choose_slave",
          "navigation": {
            "prev": "rf.test_switch",
            "next": "rf.done"
          }
        },
        {
          "id": "rf.program",
